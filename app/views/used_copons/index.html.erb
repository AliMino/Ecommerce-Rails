<p id="notice"><%= notice %></p>

<h1>Used Copons</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @used_copons.each do |used_copon| %>
      <tr>
        <td><%= link_to 'Show', used_copon %></td>
        <td><%= link_to 'Edit', edit_used_copon_path(used_copon) %></td>
        <td><%= link_to 'Destroy', used_copon, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Used Copon', new_used_copon_path %>
